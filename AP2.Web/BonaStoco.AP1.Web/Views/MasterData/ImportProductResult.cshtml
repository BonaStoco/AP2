@model BonaStoco.AP1.Web.Models.ImportProductResponse
@{
    ViewBag.Title = "Import Barang";
}
<div>
@if (!Model.HasError)
{
    <div>
        <h2>Import barang berhasil. @Html.ActionLink("Kembali ke import barang.", "ImportProduct")</h2>
    </div>
}
else
{
    <div>
        @Html.ActionLink("Kembali ke import barang.", "ImportProduct")
        <br />
        <table>
            <caption>Import barang yang gagal:</caption>
            <thead>
                <tr>
                    <th>Pesan error</th>
                </tr>
            </thead>
            <tbody>
                @foreach (string errMsg in Model.ErrorMessages)
                {
                    <tr>
                        <td>@errMsg</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
</div>