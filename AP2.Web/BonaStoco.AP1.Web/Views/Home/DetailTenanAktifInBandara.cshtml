@using BonaStoco.AP1.MasterData.Models
@model IList<DetailTenanAktif>
 <table width="100%">
    <thead>
        <tr>
            <th>Tenan ID</th>
            <th>Nama Tenan</th>
            <th>Gate</th>
        </tr>
    </thead>
    <tbody id="searchResults">
            @foreach (var item in Model)
            {
                 <tr>
                    <td>@Html.DisplayFor(m => item.TenanId)</td>
                    <td>@Html.DisplayFor(m => item.TenanName)</td>
                    <td>@Html.DisplayFor(m => item.Gate)</td>
                </tr>
            }
           
    </tbody>
    </table>