@model IList<BonaStoco.AP1.MasterData.Models.RequestProduct>
 @{
		  var no = 1;
		  var count=0;
	   }
  
	  @if(Model.Count != 0)
	  {
	  foreach (var item in Model)
	  {
		<tr>
			<td>@no</td>
			<td>@item.Nama</td>
			@if(item.Status == 0) {<td>Pending</td>}
			else if(item.Status == 1) {<td>Approved</td>}
                else if(item.Status == 2) {<td>Rejected</td>}
		</tr>

  count =count+1;
		  no++;   
        
		}
		}