@{
    var TimeSessionTimOut = 900;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <!--<script src="@Url.Content("~/Scripts/printJzebra.js")" type="text/javascript"></script>-->
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ReportSale.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")"  rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")"  rel="stylesheet" type="text/css" />
    @RenderSection("styles", false)
</head>
<body onkeypress="RefreshSessionTimeOut(@TimeSessionTimOut)" onmousemove="RefreshSessionTimeOut(@TimeSessionTimOut)">
    <div class="page">
        <header>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="Logo">
                <img src="@Url.Content("~/Content/images/AP1-Logo.png")" height="72px"/>
            </div>
            <div id="title">
                 @Html.Partial("_TitlePartial")
            </div>

            <nav>
                <div id="menu">
                        <ul id="menustyle">
                            <li>@Html.ActionLink("Home", "AP1Page", "Home")</li>  
                            <li>@Html.ActionLink("Pengiriman Barang", "ViewTenan", "PengirimanBarangAP1")</li>
                            <li><a href="javascript:void(0)" >Verifikasi</a>
                                <ul>
                                    <li>@Html.ActionLink("Barang Baru", "Index", "RequestProduct")</li>
                                    <li>@Html.ActionLink("Pengiriman Barang", "Index", "VerifikasiPengirimanBarang")</li>
                                    <li>@Html.ActionLink("Retur Barang", "Index", "VerifikasiReturBarang")</li>
                                    <li>@Html.ActionLink("Perubahan Barang", "Index", "PerubahanBarang")</li>
                                    @*<li>@Html.ActionLink("Semua Bandara", "ReportSaleSemuaBandara", "ReportSale")</li>*@
                                </ul>
                            </li>
                            <li id="aktif">@Html.ActionLink("Daftar Penerimaan", "Index", "DaftarPenerimaanAP1")</li>
                            <li><a href="javascript:void(0)" >Laporan Penjualan</a>
                                <ul>
                                    <li>@Html.ActionLink("Penjualan Total", "PenjualanTotal", "ReportSale")</li>
                                    <li>@Html.ActionLink("Rekap Per Tenant", "RekapPerTenant", "ReportSale")</li>
                                    <li>@Html.ActionLink("Detail Per Tenant", "DetailPerTenant", "ReportSale")</li>
                                    @*<li>@Html.ActionLink("Semua Bandara", "ReportSaleSemuaBandara", "ReportSale")</li>*@
                                </ul>
                            </li>
                            <li><a href="javascript:void(0)" >Faktur</a>
                                <ul>
                                    <li>@Html.ActionLink("Faktur AP1", "Index", "APReport")</li>
                                    <li>@Html.ActionLink("Faktur Pajak", "ShowDialogReport", "FakturPajak")</li>
                                    <li>@Html.ActionLink("Laporan Konsesi", "Index", "LaporanProduksi")</li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("Kartu Stok", "ReportStock", "ReportStock")</li>
                            <li>@Html.ActionLink("Exchange Rate", "Index", "ExchangeRate")</li>
                            <li>@Html.ActionLink("Data Tenan", "Index", "TarifTenan")</li>
                            <li>@Html.ActionLink("Daftar Pembayaran Tenan", "Index", "DaftarPembayaranTenanAP")</li>
                            <li><a href="javascript:void(0)" >&nbsp;&nbsp;&nbsp; Master Data&nbsp;&nbsp;&nbsp; </a>
                                <ul>
                                    <li>@Html.ActionLink("Impor Barang", "APImportData", "MasterData")</li>
                                    <li>@Html.ActionLink("Sinkronisasi Barang ", "Index", "SynchronizationPart")</li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("E-POS DPS", "Index", "DisplayMonitoringEPOS")</li>  
                             <li>@Html.ActionLink("Stock Opname", "Index", "StockOpname")</li>
                             <li>@Html.ActionLink("Laporan Void", "Index", "LaporanVoidAP")</li>
                             <li>@Html.ActionLink("Laporan Inventory", "ReportInventoryAP", "ReportInventory")</li>                         
                           @* <li>@Html.ActionLink("Monitoring", "MonitoringDashBoard", "MonitoringDashBoard")</li>*@
                        </ul>
                  </div>
              </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer-left>
            © 2011 AP1.Bonastoco.com - Telkom Indonesia
        </footer-left>
        <footer-right>
            Powered By <a href="http://www.bonastoco.com" target="_blank">Inforsys Indonesia</href>
        </footer-right>
    </div>
    @Content.Script("jquery-1.5.1.js", @Url)
    @Content.Script("jquery-1.5.1.min.js", @Url)
    @Content.Script("jquery.quicksearch.js", @Url)
    @Content.Script("modernizr-1.7.min.js", @Url)
    @Content.Script("jquery.unobtrusive-ajax.min.js", @Url)
    @Content.Script("jquery-ui-1.8.11.min.js", @Url)
    @Content.Script("jquery-ui-1.8.14.custom.min.js", @Url)
    @Content.Script("ap1.js", @Url)
    @Content.Script("SessionTimeOut.js", @Url)
    @RenderSection("scripts", false)
</body>
</html>
<div id="timeCounter" style="display: none;">@TimeSessionTimOut</div>
