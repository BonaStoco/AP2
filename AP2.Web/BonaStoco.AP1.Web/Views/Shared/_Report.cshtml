<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ReportSale.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ReportFakturPajak.css")" rel="stylesheet" type="text/css" />
    @*<link href="../../Content/kalenderView.css" rel="stylesheet" type="text/css" />*@

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                @Html.Partial("_TitlePartial")
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <nav>
                <div id="menu">
                <ul id="menustyle">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Pengiriman barang", "ViewTenan", "PengirimanBarangAP1")</li>
                    <li>@Html.ActionLink("Verifikasi Pengiriman barang", "Index", "VerifikasiPengirimanBarang")</li>
                    <li>@Html.ActionLink("Laporan Penjualan", "PageHari", "ReportSale")</li>
                    <li>@Html.ActionLink("Faktur AP1","Index","APReport")</li>
                    <li>@Html.ActionLink("Faktur Pajak", "ShowDialogReport", "FakturPajak")</li>
                    <li>@Html.ActionLink("Kartu Stok", "ReportStock", "ReportStock")</li>
                </ul></div>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
