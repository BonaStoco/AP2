@{
    var TimeSessionTimOut = 900;
 }
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
   @* <script src="@Url.Content("~/Scripts/printJzebra.js")" type="text/javascript"></script>*@
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ReportSale.css")" rel="stylesheet" type="text/css" />
 @*   <link href="@Url.Content("~/Content/ReportFakturPajak.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")"  rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")"  rel="stylesheet" type="text/css" />
    @RenderSection("styles", false)
</head>
<body onkeypress="RefreshSessionTimeOut(@TimeSessionTimOut)" onmousemove="RefreshSessionTimeOut(@TimeSessionTimOut)">
    <div class="page">
        <header>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="Logo">
                <img src="@Url.Content("~/Content/images/ap2.png")" height="72px"/>
            </div>
            <div id="title">
                 @Html.Partial("_TitlePartial")
            </div>

            <nav>
                <div id="menu">
                        <ul id="menustyle">
                            <li>@Html.ActionLink("Home", "AP2SubTerminalPage", "Home")</li>  
                            <li><a href="javascript:void(0)" >Laporan Penjualan</a>
                                <ul>
                                    <li>@Html.ActionLink("Penjualan Total", "PenjualanTotal", "ReportSale")</li>
                                    <li>@Html.ActionLink("Rekap Per Tenant", "RekapPerTenant", "ReportSale")</li>
                                    <li>@Html.ActionLink("Detail Per Tenant", "DetailPerTenant", "ReportSale")</li>
                                    @*<li>@Html.ActionLink("Semua Bandara", "ReportSaleSemuaBandara", "ReportSale")</li>*@
                                </ul>
                            </li>
                            <li>@Html.ActionLink("Faktur AP2","Index","AP2Report")</li>
                            <li>@Html.ActionLink("Faktur Pajak", "ShowDialogReport", "FakturPajakAP2")</li>
                            <li>@Html.ActionLink("Data Tenan", "Index", "TarifTenan")</li>
                            <li>@Html.ActionLink("Daftar Pembayaran Tenan", "Index", "DaftarPembayaranTenanAP")</li>
                        </ul>
                  </div>
              </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer-left>
            © 2011 AP2.Bonastoco.com - Telkom Indonesia
        </footer-left>
        <footer-right>
            Powered By <a href="http://www.bonastoco.com" target="_blank">Inforsys Indonesia</href>
        </footer-right>
    </div>
    @Content.Script("jquery-1.5.1.js", @Url)
    @Content.Script("jquery-1.5.1.min.js", @Url)
    @Content.Script("jquery.quicksearch.js", @Url)
    @Content.Script("modernizr-1.7.min.js", @Url)
    @Content.Script("jquery.unobtrusive-ajax.min.js", @Url)
    @Content.Script("jquery-ui-1.8.11.min.js", @Url)
    @Content.Script("jquery-ui-1.8.14.custom.min.js", @Url)
    @Content.Script("ap1.js", @Url)  
    @RenderSection("scripts", false)
    @Content.Script("SessionTimeOut.js", @Url)
</body>
</html>
<div id="timeCounter" style="display:none;">@TimeSessionTimOut</div>